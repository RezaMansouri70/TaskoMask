{
  "SQL": {
    "Connection": "Replace-With-Production-Connection",
    "DatabaseName": "Identity_DB"
  },
  "RabbitMQ": {
    "Host": "localhost",
    "ExchangeName": "identity-api",
    "UserName": "guest",
    "Password": "guest"
  },
  "Redis": {
    "Connection": "YourServer:YourPort",
    "Password": "Password",
    "KeyNamespace": "TaskoMaskEventStore"
  },
  "Caching": {
    "CacheTimeInMinutes": 60,
    "Enabled": true
  },
  "Identity": {
    "SuperUser": {
      "UserName": "admin",
      "Email": "admin@taskomask.ir",
      "Password": "1234"
    },
    "Options": {
      "Password": {
        "RequireLowercase": false,
        "RequireUppercase": false,
        "RequireNonAlphanumeric": false,
        "RequiredLength": 4,
        "RequireDigit": false
      },
      "Lockout": {
        "AllowedForNewUsers": false,
        "DefaultLockoutTimeSpan": 30,
        "MaxFailedAccessAttempts": 10
      },
      "User": {
        "RequireUniqueEmail": true
      }
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Exceptions" ],
    "Enrich": [ "WithExceptionDetails" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Error"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": { "expression": "@mt = 'An unhandled exception has occurred while executing the request.'" }
      }
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/Logs.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  }
}
