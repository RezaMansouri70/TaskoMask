{
  "Url": {
    "Website": "http://taskomask.ir",
    "UserPanel": "http://panel.taskomask.ir",
    "ApiGateway": "http://api.taskomask.ir"
  },
  "oidc": {
    "Authority": "https://identity.taskomask.ir",
    "ClientId": "UserPanel",
    "ResponseType": "code",
    "DefaultScopes": [
      "openid",
      "profile",
      "monolith.read",
      "monolith.write",
      "owners.read",
      "owners.write",
      "boards.read",
      "boards.write",
      "tasks.read",
      "tasks.write"
    ],
    "PostLogoutRedirectUri": "authentication/logout-callback/",
    "RedirectUri": "authentication/login-callback/"
  }
}
